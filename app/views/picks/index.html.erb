<% unless @picks.empty? %>
  <%= render "selected" %>
<% end %>

<h1>Listing matchups for Week <%= @week_num %></h1>

<%= render "weeks" %>

<table>
  <tr>
    <th>Matchup</th>
    <th>Winner</th>
    <th>Time</th>
  </tr>

<% @matchups.each do |matchup| %>
  <tr>
    <td><%= link_to getFullName(matchup.away), :controller => "picks", :action => "new", :week => @week_num, :team_id => matchup.away %> @ 
    <%= link_to getFullName(matchup.home), :controller => "picks", :action => "new", :week => @week_num, :team_id => matchup.home %></td>
    
        <% if Team.find_by_id(matchup.winner)%>
    <td><%= getFullName(matchup.winner) %></td>
        <% else %>
        <td>TBD</td>
        <% end %>
        
    <td><%= matchup.time %> TBD </td>
  </tr>
<% end %>
</table>